public class Auto {
  // Anfang Attribute
  private double laenge = 0.0;

  // Ende Attribute
  public Auto(double laenge) {
    this.laenge = laenge;
  }

  // Anfang Methoden
  public double getLaenge() {
    return laenge;
  }

  // Ende Methoden
} // end of Auto
